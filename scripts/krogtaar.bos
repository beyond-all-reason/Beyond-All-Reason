Create()
{
	bMoving = FALSE;
	bAiming = FALSE;
	Static_Var_4 = 1;
	hide rgunflare;
	hide lgunflare;
	gun_3 = pelvis;
	Static_Var_7 = 0;
	restore_delay = 2000;
	start-script MotionControl();
	start-script SmokeUnit();
	return (0);
}

