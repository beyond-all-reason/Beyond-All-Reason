	turn aimy1 to x-axis <-1.2> now;
	turn aimy1 to x-axis <0> speed <1.5>;
	if( ShootingArm )
	{
		turn luparm to x-axis <15.0> speed <180.0>;
		turn larm to x-axis <-105.0> speed <180.0>;
		sleep 300;
		turn larm to x-axis <-86.0> speed <56.0>;
		turn luparm to x-axis <0.0> speed <40.0>;
	}
	else
	{
		turn ruparm to x-axis <15.0> speed <180.0>;
		turn rarm to x-axis <-105.0> speed <180.0>;
		sleep 350;
		turn rarm to x-axis <-86.0> speed <56.0>;
		turn ruparm to x-axis <0.0> speed <40.0>;
	}
	ShootingArm = !ShootingArm;
}

