AimWeapon1(heading, pitch)
{

	start-script Open();
	signal SIGNAL_BODY;
	set-signal-mask SIGNAL_BODY;
	
	if( !HandsUp )
	{
		return (0);
	}
	turn lshoulder to x-axis <0.0> - pitch speed <150>;
	turn rshoulder to x-axis <0.0> - pitch speed <150>;	

	call-script CATT1_Aim(heading,pitch);
	start-script RestoreBody();
	return (1);
}

