Create()
{
	hide lfirept;
	hide rfirept;
	hide aimx1;
	hide aimy1;
	hide rfirept;
	hide lfirept;
	isMoving = FALSE;
	isAiming = 0;
	wpnheading = 0;
	animSpeed = 4;
	gun_1 = 0;
}

StartMoving(reversing)
{
	isMoving = TRUE;
	signal SIGNAL_MOVE;
	set-signal-mask SIGNAL_MOVE;
	start-script Walk();
	start-script UnitSpeed();
}

StopMoving()
{
	signal SIGNAL_MOVE;
	isMoving = FALSE;
	call-script StopWalking();
}



