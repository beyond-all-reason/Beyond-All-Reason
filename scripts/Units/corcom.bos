StopBuilding()
{
	hide nanospray;
	bIsBuilding = FALSE;
	set INBUILDSTANCE to 0;
	hide cagelight_emit;
	turn cagelight_emit to y-axis <0> speed <1000>;
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	call-script RestorePosition();
	return (0);
}

