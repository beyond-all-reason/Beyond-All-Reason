RestorePosition()
{
	gun_3 = torso;
	set-signal-mask SIG_AIM;
	sleep 2000;
	turn aimy1 to y-axis <0.000000> speed <90.000000>;
	turn aimx1 to x-axis <0> speed <90>;
	//wait-for-turn aimy1 around y-axis;
	bAiming = FALSE;
	// set last headings and pitches to large negative numbers
	last_primary_heading = -1000000;
	last_primary_pitch = -1000000;
	return (0);
}

ResumeBuilding()
{
	show nanospray;
	sleep 400;
	if( bIsBuilding AND ( !gun_2 ) )
	{
	turn aimy1 to y-axis buildheading - <20> speed <300.000000>;
	turn aimx1 to x-axis <-20.000000> - buildpitch speed <90.000000>;
	}
	return (0);
}

