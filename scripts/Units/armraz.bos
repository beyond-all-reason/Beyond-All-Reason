	pieceIndex = torso;
}

RestoreAfterDelay()
{
	set-signal-mask SIGNAL_AIM1;
	sleep 3000;
	start-script ExecuteRestoreAfterDelay();
}

static-var  Stunned;
ExecuteRestoreAfterDelay()
{
    if (Stunned) {
        return (1);
    }
    
	start-script CATT1_Restore();
	isAiming = FALSE;
}

AimWeapon1(heading, pitch)
{
	signal SIGNAL_AIM1;
	set-signal-mask SIGNAL_AIM1;
	spin rbarrel around z-axis speed <1000>;
	spin lbarrel around z-axis speed <-1000>;
	turn luparm to y-axis <5.0> speed <120.0>;
	turn ruparm to y-axis <5.0> speed <120.0>;

	call-script CATT1_Aim(heading,pitch);
	isAiming = TRUE;
	
	stop-spin rbarrel around z-axis decelerate <10>;
	stop-spin lbarrel around z-axis decelerate <10>;
	
	start-script RestoreAfterDelay();

	return (1);
}

FireWeapon1()
{
	
	if( !gun_1 )
	{
		turn luparm to x-axis <10.0> speed <40.0>;
		turn lcannon to x-axis <-16.0> speed <80.0>;
		wait-for-turn luparm around x-axis;
		sleep 100;
		turn luparm to x-axis <-2.0> speed <20.0>;
		turn lcannon to x-axis <-2.0> speed <30.0>;
		
		call-script lua_UnitScriptLight(3, shotcount);
		call-script lua_UnitScriptLight(4, shotcount);
	}
	else
	{
		turn ruparm to x-axis <8.0> speed <40.0>;
		turn rcannon to x-axis <-16.0> speed <80.0>;
		wait-for-turn ruparm around x-axis;
		sleep 100;
		turn ruparm to x-axis <-2.0> speed <20.0>;
		turn rcannon to x-axis <-2.0> speed <30.0>;
		
		call-script lua_UnitScriptLight(5, shotcount);
		call-script lua_UnitScriptLight(6, shotcount);
	}
}

EndBurst1()
{
	gun_1 = !gun_1;
}

QueryWeapon1(pieceIndex)
{
	// Called after each Shot
	
	pieceIndex = lflare + gun_1;
}

