		return(corpsetype);
	}
	if( severity <= 99 )
	{
		corpsetype = 3 ;
		// explode base type BITMAPONLY | NOHEATCLOUD;
		// explode turreta type EXPLODE_ON_HIT | SMOKE | FALL | NOHEATCLOUD;
