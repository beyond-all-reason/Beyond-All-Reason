		return(corpsetype);
	}
	if( severity <= 50 )
	{
		corpsetype = 2 ;
		// explode base type BITMAPONLY | NOHEATCLOUD;
		// explode turreta type FIRE | SMOKE | FALL | NOHEATCLOUD;
		// explode sleevea type FALL | NOHEATCLOUD;
