	{
		corpsetype = 1 ;
		// explode base type BITMAPONLY | NOHEATCLOUD;
		// explode turreta type BITMAPONLY | NOHEATCLOUD;
		// explode sleevea type BITMAPONLY | NOHEATCLOUD;
