Create()
{
	hide flare;
	dont-cache flare;
	bMoving = FALSE;
	bAiming = FALSE;
	bSteering = FALSE;
	Static_Var_3 = 1;
	oldHeading = get HEADING;
	gun_1 = body;
	start-script SmokeUnit();
	start-script Steering();
	start-script MotionControl();
	turn lfhip to y-axis <30> now;
	turn rfhip to y-axis <-30> now;
	turn lbhip to y-axis <-54> now;
	turn rbhip to y-axis <54> now;
	speedmult = 70;
}

