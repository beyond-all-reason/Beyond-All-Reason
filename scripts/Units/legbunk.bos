static-var leg_clearance;
dontgetyourlegshotoff(h) {
	if ( h > <165> ) { leg_clearance = <25>; } 
	else if ( h > <135> ) { leg_clearance = <15>; }
	else { leg_clearance = <0>; }
	turn CATT2_PIECE_Y to z-axis leg_clearance speed <120>;
}

