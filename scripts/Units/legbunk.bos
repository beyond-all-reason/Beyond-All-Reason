//Railgun
AimWeapon3(heading, pitch) {
	var new_heading;
	new_heading = heading - torsofunkyHeading;
	if (ANGLE_DIFFERENCE_GREATER_THAN(new_heading,<135>)) {
		return (0);
	}
	signal SIG_AIM_GUN;
	set-signal-mask SIG_AIM_GUN;
	isAiming = TRUE;
	
	oldHeadingRailgun = heading;

	turn railgun to x-axis <0> - pitch/2 speed <30>;
	start-script dontgetyourlegshotoff(heading);
	
	call-script CATT2_Aim(new_heading,(pitch+400)/2);
	start-script CATT2_Restore();
	return(1);}

//Turns the right arm up a bit to avoid visual clipping at extreme angles.
static-var leg_clearance;
dontgetyourlegshotoff(h) {
	if ( h > <165> ) { leg_clearance = <25>; } 
	else if ( h > <135> ) { leg_clearance = <15>; }
	else { leg_clearance = <0>; }
	turn CATT2_PIECE_Y to z-axis leg_clearance speed <120>;
	sleep CATT2_RESTORE_DELAY;
	turn CATT2_PIECE_Y to z-axis <0> speed <60>;
}

FireWeapon3(){
	signal SIG_AIM_GUN;
	call-script Recoil_Gun();
	return (1);}

Recoil_Gun() {
	move railgunfingers to z-axis [-10] speed [100];
	turn rarm to x-axis <25> speed [6.25];
	turn rarmorshoulder to x-axis <-25> speed [6.25];
	turn rforearm to x-axis <-25> speed [6.25];
	start-script FireGunExhausts();
	sleep 400;
	turn rarm to x-axis <0> speed [0.4];
	turn rarmorshoulder to x-axis <0> speed [0.4];
	turn rforearm to x-axis <0> speed [0.4];
	sleep 100;
	move railgunfingers to z-axis [0] speed [10.0];
}

Recoil_Bunker() {
	move piledriver to z-axis [-2] speed [20];
	turn piledriver to x-axis <10> speed [4.125];
	turn larm to x-axis <25> speed [6.25];
	turn larmorshoulder to x-axis <-25> speed [6.25];
	turn lforearm to x-axis <-25> speed [6.25];
	start-script FireBunkerExhausts();
	sleep 400;
	move piledriver to z-axis [0.0] speed [8.0];
	turn piledriver to x-axis <0> speed [0.2];
	turn larm to x-axis <0> speed [0.4];
	turn larmorshoulder to x-axis <0> speed [0.4];
	turn lforearm to x-axis <0> speed [0.4];
	sleep 100;}

RestoreAfterDelay(){
	sleep restore_delay;
	start-script ExecuteRestoreAfterDelay();}

ExecuteRestoreAfterDelay() {
    if (stunned) {
        return (1);
    }
	signal SIGNAL_RESTORE;
	set-signal-mask SIGNAL_RESTORE;
	
	isAiming = FALSE;
	turn aimy1 to y-axis <0.000000> speed <60.000000>;
	turn piledriver to x-axis <25> speed <60.000000>;
	turn railgun to x-axis <25> speed <60.000000>;
	
	start-script Retract_bunker();
	
	oldHeadingBunker = 0;
	oldHeadingRailgun = 0;

	oldHeadingTS = 0;}

FireBunkerExhausts(){
	emit-sfx 1024 + 0 from flaresmoke1;
	emit-sfx 1024 + 0 from flaresmoke2;
	return (1);}

FireGunExhausts() {
	if(fuelSaver){
		emit-sfx 1024 + 0 from flaresmokegun1;
		emit-sfx 1024 + 0 from flaresmokegun3;
		emit-sfx 1024 + 0 from flaresmokegun5;
	} else {
		emit-sfx 1024 + 0 from flaresmokegun2;
		emit-sfx 1024 + 0 from flaresmokegun4;
		emit-sfx 1024 + 0 from flaresmokegun6;
	}
	fuelSaver = !fuelSaver;
	return (1);}

