//When the Driver turns stupid and pouty and doesn't reset rotation during combat without targets
ResetWeapon() {
	set-signal-mask SIG_FIRE_BUNKER; //Set this to whichever signal the weapon emits when firing!
	sleep CATT1_RESTORE_DELAY;
	signal SIG_AIM_BUNKER;
	oldHeadingBunker = 0;
	turn aimxarml to x-axis <0.000000> speed <60.000000>;
	turn aimyarml to y-axis <0.000000> speed <60.000000>;}


RestoreAfterDelay(){
	set-signal-mask SIG_AIM_TC;
	set-signal-mask SIG_AIM_BUNKER;
	set-signal-mask SIG_AIM_GUN;
	sleep restore_delay;
	start-script ExecuteRestoreAfterDelay();}

ExecuteRestoreAfterDelay() {
    if (stunned) {
        return (1);
    }
	signal SIG_AIM_BUNKER;
	signal SIG_AIM_GUN;

	turn aimy1 to y-axis <0.000000> speed <60.000000>;
	turn aimxarml to x-axis <0.000000> speed <60.000000>;
	turn aimyarml to y-axis <0.000000> speed <60.000000>;
	turn aimxarmr to x-axis <0.000000> speed <60.000000>;
	turn aimyarmr to y-axis <0.000000> speed <60.000000>;
	turn CATT2_PIECE_Y to z-axis <0.000000> speed <60.000000>;
	

	turn piledriver to x-axis <25> speed <60.000000>;
	turn railgun to x-axis <25> speed <60.000000>;

	call-script Retract_bunker();
	wait-for-turn aimy1 around y-axis;
	wait-for-turn aimxarml around x-axis;
	wait-for-turn aimyarml around y-axis;
	wait-for-turn aimxarmr around x-axis;
	wait-for-turn aimyarmr around y-axis;

	isAiming = FALSE;

	oldHeadingBunker = 0;
	oldHeadingRailgun = 0;

	oldHeadingTS = 0xbadface;}

