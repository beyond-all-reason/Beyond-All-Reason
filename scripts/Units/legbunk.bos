//Targetting System / Responsible for torso rotation and saving that info for the other turrets.
AimWeapon1(heading, pitch)
{
	signal SIG_AIM_TC;
	set-signal-mask SIG_AIM_TC;
	call-script StopPushups();
	var cauaim;
    var cauface;
	if ((((oldHeadingBunker > 0) AND (oldHeadingRailgun < 0) AND (oldHeadingBunker > 15000) AND (oldHeadingRailgun < (-15000))) OR ((oldHeadingBunker < 0) AND (oldHeadingRailgun > 0) AND (oldHeadingBunker < (-15000)) AND (oldHeadingRailgun > 15000))) AND (15000 > get ABS(cauface - cauaim)))
	{
		if ((oldHeadingBunker+oldHeadingRailgun)/2 < 0)
		{
			if (isturning == 1)
			{
				turn aimy1 to y-axis <179> speed <160.0000> ;
			}
			if (isturning == 0)
			{
				turn aimy1 to y-axis <179> speed <120.0000> ;
			}
		}
		if ((oldHeadingBunker+oldHeadingRailgun)/2 > 0)
		{
			if (isturning == 1)
			{
				turn aimy1 to y-axis <-179> speed <160.0000> ;
			}
			if (isturning == 0)
			{
				turn aimy1 to y-axis <-179> speed <120.0000> ;
			}
		}
	}
	else {
		if (isturning == 1)
		{
			turn aimy1 to y-axis (oldHeadingBunker+oldHeadingRailgun)/4 + heading/2 speed <160.0000> ;
		}
		if (isturning == 0)
		{
			turn aimy1 to y-axis (oldHeadingBunker+oldHeadingRailgun)/4 + heading/2 speed <120.0000> ;
		}
	}

	//We will use the torso's origin and a parented empty placed in front to get this piece's heading
	var torso_xz;
    torso_xz = get PIECE_XZ(torso);
    var torso_x;
    torso_x = (torso_xz & 0xffff0000) / 0x00010000;                //unpack x coordinates of torso (credit for unpack code goes to Beherith)
    var torso_z;
    torso_z = (torso_xz & 0x0000ffff);                            //unpack z coordinates of torso

    var turret_xz;
    turret_xz = get PIECE_XZ(frontTorso);
    var turret_x;
    turret_x = (turret_xz & 0xffff0000) / 0x00010000;        //unpack x coordinates of turret
    var turret_z;
    turret_z = (turret_xz & 0x0000ffff);                    //unpack z coordinates of turret

    var aim_x;
    var aim_z;
    aim_x = torso_x - turret_x;                                //calculate relative coordinates of the torso to the turret (how far away it is in each direction)
    aim_z = torso_z - turret_z;

	//And this will get direction of the pelvis/base in worldspace
    var hull_xz;
    hull_xz = get PIECE_XZ(pelvis);
    var hull_x;
    hull_x = (hull_xz & 0xffff0000) / 0x00010000;
    var hull_z;
    hull_z = (hull_xz & 0x0000ffff);

    var smoke_xz;
    smoke_xz = get PIECE_XZ(frontPelvis);
    var smoke_x;
    smoke_x = (smoke_xz & 0xffff0000) / 0x00010000;
    var smoke_z;
    smoke_z = (smoke_xz & 0x0000ffff);

    var face_x;
    var face_z;
    face_x = hull_x - smoke_x;                                //calculate relative coordinates of the hull to the smoke (how far away it is in each direction)
    face_z = hull_z - smoke_z;


    cauaim = get ATAN(aim_z, aim_x);                        //use arctan (ATAN) to calculate the angle of the turret. The result is in cau, which is the unit this code format uses for angles. Its up to 32768 (180°), positive or negative depending on direction
    cauface = get ATAN(face_z, face_x);                        //use arctan (ATAN) to calculate the angle of the hull. The result is in cau, which is the unit this code format uses for angles. Its up to 32768 (180°), positive or negative depending on direction

    torsofunkyHeading = (cauface - cauaim) +400;                                //calculate angle of turret relative to the hull
	if (((get ABS(oldHeadingTS - heading)) > 65536) OR(((get ABS(oldHeadingTS - heading)) > 1300) AND ((get ABS(oldHeadingTS - heading)) < 64236)))
	{
		oldHeadingTS = 1000000;
		wait-for-turn aimy1 around y-axis;
	}
	oldHeadingTS = heading;
	isAiming = TRUE;
    start-script RestoreAfterDelay();
	return (1);
}




//Pile Driver
AimWeapon2(heading, pitch)
{
	var new_heading;
	new_heading = heading - torsofunkyHeading;
	if (ANGLE_DIFFERENCE_GREATER_THAN(new_heading,<135>)) {
		return (0);
	}
	signal SIG_AIM_BUNKER;
	set-signal-mask SIG_AIM_BUNKER;
	isAiming = TRUE;
	
	turn piledriver to x-axis <0> - pitch/2 speed <30>;
	oldHeadingBunker = heading;
	call-script CATT1_Aim(new_heading + <5>,(pitch+400)/2);
	start-script CATT1_Restore();
	return(1);
}

FireWeapon2()
{
	signal SIG_FIRE_BUNKER;
	call-script Attack_bunker();
	call-script Recoil_Bunker();
	sleep 100;
	call-script Retract_bunker();
}

