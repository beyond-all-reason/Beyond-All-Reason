static-var oldsteerheading, timer;
Steering(heading, currentSpeed) {
	var aimAdj;
	while (TRUE)
	{
		heading = get HEADING;
		aimAdj = (heading - oldsteerheading);
		if ((get ABS(oldsteerheading - heading)) > <1>)
		{
			isturning = 1;
		}
		else
		{
			isturning = 0;
		}
		oldsteerheading = heading;
		if (timer > 30)
		{
			timer = 0;
			start-script RestoreAfterDelay();
		}
		sleep 20;
	}}

