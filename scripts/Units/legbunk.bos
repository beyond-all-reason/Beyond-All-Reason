UnitSpeed(){ // REMEMBER TO animspeed = 2 in Create() !!
	maxSpeed = get MAX_SPEED;
	animFramesPerKeyframe = 2;
	maxSpeed = maxSpeed + (maxSpeed /(2*animFramesPerKeyframe));
	while(TRUE){
		animSpeed = (get CURRENT_SPEED);
		if (animSpeed<1) animSpeed=1;
		animSpeed = (maxSpeed * 2) / animSpeed;
		if (animSpeed<1) animSpeed=1;
		if (animspeed>4) animSpeed = 4;
		sleep 65;
	}
}
StartMoving(){
	signal SIG_WALK;
	isMoving=TRUE;
	call-script StopPushups();
	start-script Walk();}
StopMoving(){
	signal SIG_WALK;
	isMoving=FALSE;
	call-script StopWalking();
	if (!isPushing and !isAiming and !isMoving) {
			start-script Pushup();
	}
}

