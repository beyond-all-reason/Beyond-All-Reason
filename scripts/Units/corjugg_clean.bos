#include "../unit_hitbyweaponid_and_smoke.h"

Create()
{
	hide mainbarrelflare;
	hide toplaserflare;
	hide aimy1;
	hide llaserflare;
	hide rlaserflare;
	isMoving = FALSE;
	animSpeed = 5;
	mainturretheading = 0;
	restore_delay = 3000;
	stepcount = 0;
}

SetMaxReloadTime(reloadMS)
{
	restore_delay = reloadMS * 2;
}




RestoreAfterDelay()
{
	set-signal-mask 4 | 8 | 16 | 32;
	sleep restore_delay;
	turn aimy1 to y-axis <0.0> speed <50.0>;
	turn sleeve to x-axis <0.0> speed <30.0>;
	
	turn toplaser to x-axis <0.0> speed <30.0>;
	turn toplaser to y-axis <0.0> speed <30.0>;
	turn llaser to x-axis <0.0> speed <30.0>;
	turn rlaser to x-axis <0.0> speed <30.0>;
}

AimFromWeapon1(pieceIndex)
{
	pieceIndex = sleeve;
}

QueryWeapon1(pieceIndex)
{

	pieceIndex = mainbarrelflare;
}

AimWeapon1(heading, pitch)
{
	signal 4;
	set-signal-mask 4;
	mainturretheading = heading;
	turn aimy1 to y-axis heading speed <90.0>;
	turn sleeve to x-axis <0.0> - pitch speed <50.0>;
	wait-for-turn aimy1 around y-axis;
	wait-for-turn sleeve around x-axis;
	start-script RestoreAfterDelay();
	return (1);
}


FireWeapon1()
{
	move mainbarrel to z-axis [-5.0] now;
	sleep 150;
	move mainbarrel to z-axis [0.0] speed [10.0];
}

AimWeapon2(heading, pitch)
{
	signal 8;
	set-signal-mask 8;
	turn llaser to y-axis heading speed <300.071429>;
	wait-for-turn llaser around y-axis;
	return (1);
}

FireWeapon2()
{
	set CHANGE_TARGET to 2;
}

AimFromWeapon2(pieceIndex)
{
	pieceIndex = llaser;
}

QueryWeapon2(pieceIndex)
{
	pieceIndex = llaserflare;
}

