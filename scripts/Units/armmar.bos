static-var  Stunned;
ExecuteRestoreAfterDelay()
{
    if (Stunned) {
        return (1);
    }
	turn torso to y-axis <0.000000> speed <120.000000>;
	turn luparm to x-axis <0.000000> speed <120.000000>;
	turn ruparm to x-axis <0.000000> speed <120.000000>;
	turn laaturret to y-axis <0.000000> speed <120.000000>;
	turn raaturret to y-axis <0.000000> speed <120.000000>;
	bAiming = FALSE;
	wpn1_lasthead = 1000000;
	wpn2_lasthead = 1000000;
}
SetStunned(State)
{
    Stunned = State;
	if (!Stunned) {
	    start-script ExecuteRestoreAfterDelay();
	}
}
RestoreAfterDelay()
{
	set-signal-mask SIG_AIM;
	sleep restore_delay;
	start-script ExecuteRestoreAfterDelay();
}

AimPrimary(heading, pitch)
{
	signal SIG_AIM;
	bAiming = TRUE;
	turn torso to y-axis heading speed <140.000000>;
	turn luparm to x-axis 0 - pitch speed <360>;
	turn ruparm to x-axis 0 - pitch speed <360>;
	while( !Static_Var_3 )
	{
		return(0);
	}
	if (((get ABS(wpn1_lasthead - heading)) > 65536) OR(((get ABS(wpn1_lasthead - heading)) > 1300) AND ((get ABS(wpn1_lasthead - heading)) < 64236)))
	{
		wpn1_lasthead = 1000000;
		wait-for-turn torso around y-axis;
		wait-for-turn luparm around x-axis;
	}
	wpn1_lasthead = heading;
	start-script RestoreAfterDelay();
	return (1);
}

AimSecondary(heading, pitch)
{
	signal SIG_AIM_2;
	turn laaturret to y-axis heading speed <180.000000>;
	turn raaturret to y-axis heading speed <180.000000>;
	if (((get ABS(wpn2_lasthead - heading)) > 65536) OR(((get ABS(wpn2_lasthead - heading)) > 5000) AND ((get ABS(wpn2_lasthead - heading)) < 60536)))
	{
		wpn2_lasthead = 1000000;
		wait-for-turn laaturret around y-axis;
	}
	wpn2_lasthead = heading;
	start-script RestoreAfterDelay();
	return (1);
}

FirePrimary()
{
	if( gun_1 == 1 )
	{
	    emit-sfx 1024 + 0 from lflare;
		move lcannon to z-axis [-3.000000] speed [30.000000];
		sleep 150;
		move lcannon to z-axis [0.000000] speed [3.000000];
	
	}
	if( gun_1 == 2 )
	{
	    emit-sfx 1024 + 0 from rflare;
		move rcannon to z-axis [-3.000000] speed [30.000000];
		sleep 150;
		move rcannon to z-axis [0.000000] speed [3.000000];

	}
	if (gun_1 ==1) gun_1 = 2;
	else gun_1 = 1;
}

FireSecondary()
{
	if( gun_2 == 1 )
	{
	    show lflareaa;
		sleep 150;
		hide lflareaa;
	}
	if( gun_2 == 2 )
	{
	    show rflareaa;
		sleep 150;
		hide rflareaa;
	}
	if (gun_1 ==1) gun_1 = 2;
	else gun_1 = 1;
}

