		explode leftTopBarrel type BITMAPONLY | NOHEATCLOUD;
		explode leftTopChamber type BITMAPONLY | NOHEATCLOUD;
		return(corpsetype);
	}

	if( severity <= 50 )
	{
		corpsetype = 2 ;
		explode chassis type BITMAPONLY | NOHEATCLOUD;
