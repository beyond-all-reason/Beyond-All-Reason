Create()
{
	hide rfire;
	hide lfire;
	hide aimx1;
	hide aimy1;
	hide rfire;
	hide lfire;
	isMoving = FALSE;
	isAiming = FALSE;
	gun_1 = 0;
	currentSpeed = 100;
	moveSpeed = get MAX_SPEED;
	weaponReady = FALSE;
	curPitch1 = 0;
	curHead1 = 0;
	animSpeed = 2;
}

StartMoving(reversing){
	signal SIGNAL_MOVE;
	isMoving=TRUE;
	set-signal-mask SIGNAL_MOVE;
	start-script UnitSpeed();
	call-script StopAnimation();
	start-script Walk();
}
StopMoving(){
	signal SIGNAL_MOVE;
	isMoving=FALSE;
	call-script StopWalking();
	start-script Animate();
}




static-var  Stunned;
ExecuteRestoreAfterDelay()
{
    if (Stunned) {
        return (1);
    }

	turn aimy1 to y-axis <0> speed <300.0>;
	turn aimx1 to x-axis <0> speed <300.0>;
	curPitch1 = 0;
	curHead1 = 0;
	weaponReady = FALSE;
    isAiming = 0;

}
SetStunned(State)
{
    Stunned = State;
	if (!Stunned) {
	    start-script ExecuteRestoreAfterDelay();
	}
}
RestoreAfterDelay()
{
	set-signal-mask SIGNAL_AIM1;
	sleep 3000;
	start-script ExecuteRestoreAfterDelay();

}

AimFromWeapon1(pieceIndex)
{
	pieceIndex = aimy1;
}

QueryWeapon1(pieceIndex)
{
	pieceIndex = rfire + gun_1;
	dbg(rfire, gun_1, pieceIndex);
}

AimWeapon1(heading, pitch)
{	
	signal SIGNAL_AIM1;
	set-signal-mask SIGNAL_AIM1;

	var canShoot;
	var delta;
	
	while (!canShoot){
		canShoot = TRUE;
		delta = WRAPDELTA(heading - curHead1);
		if (ABSOLUTE_GREATER_THAN(delta,(Weapon1TurretY / 30))) {
			curHead1 = curHead1 + SIGN(delta) * (Weapon1TurretY / 30);
			canShoot = FALSE;
		}else{
			curHead1 = heading;
		}
		delta = WRAPDELTA(pitch - curPitch1);
		if (ABSOLUTE_GREATER_THAN(delta,(Weapon1TurretX / 30))) {
			curPitch1 = curPitch1 + SIGN(delta) * (Weapon1TurretX / 30);
			canShoot = FALSE;
		}else{
			curPitch1 = pitch;
		}
		
		turn aimy1 to y-axis curHead1 speed Weapon1TurretY;
		turn aimx1 to x-axis <0> - curPitch1 speed Weapon1TurretX;

		//get PRINT(get GAME_FRAME, curHead1, heading);

		if (!canShoot) {
			sleep 1;
		}
	}
	
	start-script RestoreAfterDelay();
	return (1);
}



FireWeapon1()
{
	//gun_1 == 0 means right!	
	if (gun_1 == 1 ){
		turn ruparm to x-axis <15> now;
		turn rloarm to x-axis <-15> now;
		turn ruparm to x-axis <0> speed <30>;
		turn rloarm to x-axis <0> speed <30>;
	}
	else
	{
		turn luparm to x-axis <15> now;
		turn lloarm to x-axis <-15> now;
		turn luparm to x-axis <0> speed <30>;
		turn lloarm to x-axis <0> speed <30>;
	}
	dbg(gun_1);
	// This sleep is very important for laser weapons. We have to wait for beamTTL frames before we swap weapons
	// Otherwise the beam will switch draw positions mid-firing
	sleep 90;
	gun_1 = !gun_1;
	
	dbg(gun_1);
}

