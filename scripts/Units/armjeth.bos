Create()
{
	hide rflare;
	hide rexhaust;
	hide lexhaust;
	hide lflare;
	hide aimx1;
	hide aimy1;
	hide rflare;
	hide lflare;
	isMoving = FALSE;
	gun_3 = torso;
	restore_delay = 3000;
	animSpeed = 3;
}

SetMaxReloadTime(reloadMS)
{
	restore_delay = reloadMS * 5;
}

static-var  Stunned;
ExecuteRestoreAfterDelay()
{
    if (Stunned) {
        return (1);
    }
	set-signal-mask 0;
	start-script OpenCloseAnim(0);
	turn aimy1 to y-axis <0.0> speed <90.0>;
	wpn2_lasthead = 0xbadface;
}
SetStunned(State)
{
    Stunned = State;
	if (!Stunned) {
	    start-script ExecuteRestoreAfterDelay();
	}
}
RestoreAfterDelay()
{
	set-signal-mask SIGNAL_AIM2;
	sleep restore_delay;
	start-script ExecuteRestoreAfterDelay();
}

AimFromWeapon1(pieceIndex)
{
	pieceIndex = torso;
}

QueryWeapon1(pieceIndex)
{
	pieceIndex = rflare + gun_3;;
}

Open(){
		turn lturret to z-axis <-45> speed <360>;
		turn rturret to z-axis <45> speed <360>;
		wait-for-turn lturret around z-axis;
}

Close(){
		turn rturret to z-axis <0> speed <360>;
		turn lturret to z-axis <0> speed <360>;
		wait-for-turn rturret around z-axis;
}

AimWeapon2(heading, pitch)
{
	start-script OpenCloseAnim(1);
	signal SIGNAL_AIM2;
	start-script RestoreAfterDelay();
	turn aimy1 to y-axis heading speed <275.0>;
	
	if (!IsOpen) {
		return 0;
	}
	// Very important note:
	// Because wait-for-turn often takes much longer than just two frames, so for units which continuous aim set
	// make sure to NOT have a signal mask for this thread!
	if ((wpn2_lasthead == 0xbadface) OR ABSOLUTE_GREATER_THAN(WRAPDELTA(heading - wpn2_lasthead), <27>))
