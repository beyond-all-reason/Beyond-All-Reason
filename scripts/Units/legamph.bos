#define BASEPIECE base
#define HITSPEED <55.0>
//how 'heavy' the unit is, on a scale of 1-10
#define UNITSIZE 5
#define MAXTILT 100

#include "unit_hitbyweaponid_and_smoke.h"

StartMoving()
{
	isMoving = TRUE;
	signal SIG_MOVE;
	set-signal-mask SIG_MOVE;
}

StopMoving()
{
	isMoving = FALSE;
	signal SIG_MOVE;
	call-script StopWalking();
}

SweetSpot(piecenum)
{
	piecenum = base;
}

SetStunned(State)
{
    Stunned = State;
}
RestoreAfterDelayGun()
{
	sleep restore_delay;
	if (Stunned) {
        return (1);
    }
	gun_lasthead = -1000000;
	gun_lastpitch = -1000000;
	call-script CloseMouth();
	gun_deploy = FALSE;
	turn neck to y-axis <0.000000> speed <90.000000>;
	turn neck to x-axis <0.000000> speed <50.000000>;
	isAiming = FALSE;
}

//Heatray
AimSecondary(heading, pitch)
{ 
	signal SIG_AIM_3;
	set-signal-mask SIG_AIM_3;
	isAiming = TRUE;
	// values in <> are degrees per second
	// otherwise angles are in COB angular unit (cau) - There are 65536 cau in a circle
	// In general, 6 cau per frame ~= 1 degree per second
	
	// checks if the mouth is deployed
	turn neck to y-axis heading speed <200.000000>;
	turn neck to x-axis <0.000000> - pitch speed <100.000000>;
	if (gun_deploy == FALSE)
	{
		//if not, move it up to the deploy position, and wait
		call-script OpenMouth();
		// after moving, set variable to true, so deploy is skipped on future aims 
		gun_deploy = TRUE;
	}
	// if the turret can turn to its new heading in one frame, just return true and do not wait for turn
	if ( (get ABS(gun_lasthead - heading)<1200) AND (get ABS(gun_lastpitch - pitch)<600) )
	{
		gun_lasthead = heading;
		gun_lastpitch = pitch;
		// start the restore to neutral script
		start-script RestoreAfterDelayGun();
		return (1);
	}
	// if turret needs more than one frame to turn to new heading, wait for the turn
	// seems to take 3 frames for wait-for-turn to process
	wait-for-turn neck around y-axis;
	wait-for-turn neck around x-axis;
	gun_lasthead = heading;
	gun_lastpitch = pitch;
	// start the restore to neutral script
	start-script RestoreAfterDelayGun();
	return (1);
}

FireSecondary()
{

}

AimFromSecondary(piecenum)
{
	piecenum = heatray;
}

QuerySecondary(piecenum)
{
	piecenum = heatrayaim;
}

RestoreAfterDelayTorp()
{
	sleep restore_delay;
	if (Stunned) {
        return (1);
    }
	torp_lasthead = -1000000;
	turn torpedolauncha to y-axis <0> speed <200.000000>;
	wait-for-turn torpedolauncha around y-axis;
	// torpedo tube has started moving down, set variable to FALSE to redeploy on next aim call
	torp_deploy = FALSE;
	move torpedolauncha to y-axis [0] speed [25];
}

AimPrimary(heading, pitch)
{
	if ( get IN_WATER(0) == 0 )
    {
     return (0);
    }	
	signal SIG_AIM;
	set-signal-mask SIG_AIM;
	// checks if the torpedo launcher is deployed
	if (torp_deploy == FALSE)
	{
		//if not, move it up to the deploy position, and wait
		move torpedolauncha to y-axis [5] speed [25];
		wait-for-move torpedolauncha along y-axis;
		// after moving, set variable to true, so deploy is skipped on future aims 
		torp_deploy = TRUE;
	}
	// values in <> are degrees per second
	// otherwise angles are in COB angular unit (cau) - There are 65536 cau in a circle
	// In general, 6 cau per frame ~= 1 degree per second
	turn torpedolauncha to y-axis heading speed <200.000000>;
	// if the turret can turn to its new heading in one frame, just return true and do not wait for turn
	if (get ABS(torp_lasthead - heading) < 1200)
	{
		torp_lasthead = heading;
		// start the restore to neutral script
		start-script RestoreAfterDelayTorp();	
		return (1);
	}
	// if turret needs more than one frame to turn to new heading, wait for the turn
	// seems to take 3 frames for wait-for-turn to process
	wait-for-turn torpedolauncha around y-axis;
	torp_lasthead = heading;
	// start the restore to neutral script
	start-script RestoreAfterDelayTorp();	
	return (1);

}

FirePrimary()
{
}

AimFromPrimary(piecenum)
{
	piecenum = torpedotube;
}

QueryPrimary(piecenum)
{
	
	piecenum = torpaim;

}

Killed(severity, corpsetype)
{
	if( severity <= 25 )
	{
		corpsetype = 1 ;
		explode base type BITMAPONLY | NOHEATCLOUD;
		explode torpedolauncha type BITMAPONLY | NOHEATCLOUD;
		return(corpsetype);
	}
	if( severity <= 50 )
	{
		corpsetype = 2 ;
		explode base type FIRE | SMOKE | FALL | NOHEATCLOUD;
		explode torpedolauncha type FIRE | SMOKE | FALL | NOHEATCLOUD;

		return(corpsetype);
	}
	if( severity <= 99 )
	{
		corpsetype = 3 ;
		explode base type FIRE | SMOKE | FALL | NOHEATCLOUD;
		explode torpedolauncha type EXPLODE_ON_HIT | SMOKE | FALL | NOHEATCLOUD;
		explode neck type EXPLODE_ON_HIT | SMOKE | FALL | NOHEATCLOUD;

		return(corpsetype);
	}
	corpsetype = 3 ;
		explode base type EXPLODE_ON_HIT | FIRE | SMOKE | FALL | NOHEATCLOUD;
		explode torpedolauncha type EXPLODE_ON_HIT | FIRE | FALL | NOHEATCLOUD;
		explode torpedotube type EXPLODE_ON_HIT | FIRE | FALL | NOHEATCLOUD;
		explode neck type EXPLODE_ON_HIT | FIRE | FALL | NOHEATCLOUD;

	return corpsetype;
}
