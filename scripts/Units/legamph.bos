Create()
{
	animspeed = 6;
	hide torpaim;
	hide heatrayaim;
	isMoving = FALSE;
	isAiming = FALSE;
	restore_delay = 2000;
	// Initializing with large negative values, to ensure first aim does wait-for-turn
	gun_lasthead = -1000000;
	torp_lasthead = -1000000;
	torp_deploy = FALSE;
	gun_deploy = FALSE;
	inWaterForm = 0;
	isInWater = FALSE;
	
	start-script Movement();
	start-script UnitSpeed();
}

Movement() {
	while( TRUE ){
		isInWater = get IN_WATER(0);
		if( !inWaterForm ){
			call-script OpenHeatsinks();
			if( isInWater == 1 ){
				call-script transform();
				call-script CloseHeatsinks();
				inWaterForm = 1;
			}
		} else {
			if( isInWater == 0 ) {
				call-script reform();
				call-script OpenHeatsinks();
				inWaterForm = 0;
			}
		}

		if( isMoving ) {
			if( inWaterForm )
				{
					call-script swim();
				} else {
					call-script walk();
				}
		}else{
			sleep 100;
		}
		
	}
	return (0);
}

