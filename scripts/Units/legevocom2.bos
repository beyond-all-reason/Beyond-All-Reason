static-var  Stunned;
SetStunned(State)
{
    Stunned = State;
	if (!Stunned) {
	    start-script ExecuteRestoreAfterDelay();
	}
}

RestoreAfterDelay()
{
	set-signal-mask SIGNAL_AIM1;
	sleep 2000;
	if (!isBuilding)
	{
		start-script ExecuteRestoreAfterDelay();
	}
}

ExecuteRestoreAfterDelay()
{
    if (Stunned) {
        return (1);
    }
	
	isAiming = 0;
	isAimingDgun = 0;
	rightarm = 1;
	leftarm = 1;
	torsoRot = 0;
	
	turn aimy1 to  y-axis <0> speed <100.0>;
	stop-spin barrelminigun around z-axis decelerate <30>;
	turn rgturret to y-axis <0> speed <30>;
	turn rgturretarm to x-axis <-5> speed <30>;
	turn rgsleeve to x-axis <-5> speed <30>;
	
	turn larm to x-axis <15> speed <95.0000>; 
	turn rarm to x-axis <15> speed <95.0000>; 
	turn lhand to x-axis <30> speed <95.0000>; 
	turn rhand to x-axis <30> speed <95.0000>; 
	
	turn lshoulder to y-axis <10> speed <95.0000>; 
	turn rshoulder to y-axis <-10> speed <95.0000>; 
}

// Call this from StopMoving()!
StopWalking() {
	animSpeed = 10; // tune restore speed here, higher values are slower restore speeds
	
	turn lthigh to x-axis <0> speed <1000.0> / animSpeed;
	turn rthigh to x-axis <0> speed <1000.0> / animSpeed;
	turn rthigh to y-axis <-15> speed <1000.0> / animSpeed;
	turn lthigh to y-axis <15> speed <1000.0> / animSpeed;
	turn rthigh to z-axis <5> speed <500.0> / animSpeed;
	turn lthigh to z-axis <-5> speed <500.0> / animSpeed;
	
	turn lleg to x-axis <0> speed <2000.0> / animSpeed;
	turn rleg to x-axis <0> speed <2000.0> / animSpeed;
	
	turn lfoot to x-axis <0> speed <1000.0> / animSpeed;
	turn rfoot to x-axis <0> speed <1000.0> / animSpeed;
	
	turn ltoes to x-axis <0> speed <2000.0> / animSpeed;
	turn rtoes to x-axis <0> speed <2000.0> / animSpeed;
	turn ltoes to y-axis <0> speed <1000.0> / animSpeed;
	turn rtoes to y-axis <0> speed <1000.0> / animSpeed;
	turn ltoes to z-axis <5> speed <200.0> / animSpeed;
	turn rtoes to z-axis <-5> speed <200.0> / animSpeed;
	
	turn pelvis to x-axis <0> speed <1000.0> / animSpeed;
	turn pelvis to y-axis <0> speed <1000.0> / animSpeed;
	move pelvis to y-axis [0] speed [100.0] / animSpeed;
	move pelvis to z-axis [0] speed [100.0] / animSpeed;
	
	turn torso to y-axis <0> speed <1000.0> / animSpeed;
	
	turn head to x-axis <0> speed <200.0> / animSpeed;
	turn head to y-axis <0> speed <200.0> / animSpeed;
	
	// Prevent animation if arm is in use
	if (leftArm == 1)
	{
		turn lshoulder to y-axis <10> speed <1000.0> / animSpeed;
		turn lshoulder to z-axis <-5> speed <1000.0> / animSpeed;
		turn larm to x-axis <15> speed <803.0> / animSpeed;
		turn lhand to x-axis <30> speed <802.0> / animSpeed;
	}
	if (rightArm == 1)
	{
		turn rshoulder to y-axis <-10> speed <1000.0> / animSpeed;
		turn rshoulder to z-axis <5> speed <1000.0> / animSpeed;
		turn rarm to x-axis <15> speed <803.0> / animSpeed;
		turn rhand to x-axis <30> speed <802.0> / animSpeed;
	}
}


