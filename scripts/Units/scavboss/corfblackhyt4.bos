Create()
{
	hide thrustr2;
	hide thrustr1;
	hide thrustl2;
	hide thrustl1;
	hide flare2;
	hide flare5;
	hide wake5;
	hide flare6;
	hide wake4;
	hide wake1;
	hide flare3a;
	hide flare3b;
	hide flare1b;
	hide flare1d;
	hide flare1c;
	hide flare1a;
	hide wake3;
	hide flare4b;
	hide flare4a;
	hide wake2;
	set ARMORED to 1;
	turn jetl1 to x-axis <-90.000000> now;
	turn jetr1 to x-axis <-90.000000> now;
	turn jetl2 to x-axis <-90.000000> now;
	turn jetr2 to x-axis <-90.000000> now;
	Static_Var_1 = 0;
	Static_Var_3 = 0;
	Static_Var_2 = 0;
	restore_delay = 3000;
	start-script SmokeUnit();
}

Activate()
{
	show thrustr2;
	show thrustr1;
	show thrustl2;
	show thrustl1;
}

Deactivate()
{
	hide thrustr2;
	hide thrustr1;
	hide thrustl2;
	hide thrustl1;
}

