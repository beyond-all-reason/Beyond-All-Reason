		explode barrel type EXPLODE_ON_HIT | FIRE | SMOKE | FALL | NOHEATCLOUD;
		return(corpsetype);
	}
	corpsetype = 3 ;
		explode pelvis type BITMAPONLY | NOHEATCLOUD;
		explode hingefl type EXPLODE_ON_HIT | FIRE | SMOKE | FALL | NOHEATCLOUD;
