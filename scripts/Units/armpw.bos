Create()
{
	hide rfire;
	hide lfire;
	hide aimx1;
	hide aimy1;
	hide rfire;
	turn rfire to x-axis <10> now;
	hide lfire;
	turn lfire to x-axis <10> now;
	isMoving = FALSE;
	isAiming = FALSE;
	gun_1 = 0;
	restore_delay = 3000;
	animSpeed = 2;
	burst1 = 1;
	bAnimate = FALSE;

	weaponReady = FALSE;

	call-script StopWalking(); // start in power stance!
}


StartMoving(reversing)
{
	signal SIGNAL_MOVE;
	set-signal-mask SIGNAL_MOVE;
	start-script UnitSpeed();
	call-script StopAnimation();
	isMoving = TRUE;
	start-script Walk();

}

StopMoving()
{
	isMoving = FALSE;
	signal SIGNAL_MOVE;
	call-script StopWalking();
	start-script Animate();
}


SetStunned(State)
{
    Stunned = State;
	if (!Stunned) {
	    start-script ExecuteRestoreAfterDelay();
	}
}
RestoreAfterDelay()
{
	set-signal-mask SIGNAL_AIM1;
	sleep restore_delay;
	start-script ExecuteRestoreAfterDelay();
}

AimFromWeapon1(pieceIndex)
{
	pieceIndex = aimx1;
}

QueryWeapon1(pieceIndex)
{
	pieceIndex = rfire + gun_1;
}

FireWeapon1()
{
	//Called whenever the trigger is pulled on a weapon. This occurs before the first shot in a burst/projectile salvo (only).
	burst1 = 1;
	gun_1 = !gun_1;
}

Shot1()
{
	//Called before every shot a weapon fires. This includes burst and projectile tags. This happens after FireWeaponX(), even for the first shot
	if( gun_1 )
	{
		emit-sfx 1024 + 0 from lfire;
		move lgun to z-axis [-0.5] now;
		move lgun to z-axis [0] speed [2];
		turn lloarm to x-axis <-2.5> * burst1 now;
		turn lloarm to x-axis <0> speed <10>;
		turn luparm to x-axis <2.5> * burst1 now;
		turn luparm to x-axis <0> speed <10>;
	}
	else
	{
		emit-sfx 1024 + 0 from rfire;
		move rgun to z-axis [-0.5] now;
		move rgun to z-axis [0] speed [2];
		turn rloarm to x-axis <-2.5> * burst1 now;
		turn rloarm to x-axis <0> speed <10>;
		turn ruparm to x-axis <2.5> * burst1 now;
		turn ruparm to x-axis <0> speed <10>;
	}
	burst1 = burst1 + 1;
}


DrawWeapon1()
{
		isAiming = TRUE;

		turn ruparm to x-axis <0> speed <300.0>;
		turn ruparm to x-axis <0> speed <300.0>;
		turn lloarm to x-axis <0> speed <300.0>;
		turn rloarm to x-axis <0> speed <300.0>;
		
		wait-for-turn lloarm around x-axis;
		wait-for-turn rloarm around x-axis;
		wait-for-turn luparm around x-axis;
		wait-for-turn ruparm around x-axis;
	
		weaponReady = TRUE;
}

AimWeapon1(heading, pitch)
{

	signal SIGNAL_AIM1; // Teminates other aim threads, but not ready threads

	if (bAnimate) { // Stop doing pushups
		call-script StopAnimation();
	}

	// The usual problem here is the following:
	// New threads are spawned every REAIMTIME (two) frames, and run before the queued older ones
	// Due to the weapon ready check, many threads get added in, and then all start running when the weapon is ready

	if (!weaponReady) {
		start-script DrawWeapon1();
	}

	set-signal-mask SIGNAL_AIM1;

	var canShoot;
	var delta;
	
	while (!canShoot){
		canShoot = TRUE;
		delta = WRAPDELTA(heading - curHead1);
		if (ABSOLUTE_GREATER_THAN(delta,(Weapon1TurretY / 30))) {
			curHead1 = curHead1 + SIGN(delta) * (Weapon1TurretY / 30);
			canShoot = FALSE;
		}else{
			curHead1 = heading;
		}
		delta = WRAPDELTA(pitch - curPitch1);
		if (ABSOLUTE_GREATER_THAN(delta,(Weapon1TurretX / 30))) {
			curPitch1 = curPitch1 + SIGN(delta) * (Weapon1TurretX / 30);
			canShoot = FALSE;
		}else{
			curPitch1 = pitch;
		}
		 
		// Fun note, NOW does not override preexisting turn with other speed
		turn aimy1 to y-axis curHead1 speed Weapon1TurretY;
		turn aimx1 to x-axis <0> - curPitch1 speed Weapon1TurretX;
		
		if (!canShoot) {
			sleep 32;
		}
	}
	
	start-script RestoreAfterDelay();
	return (weaponReady);
}

