Create()
{
	hide rexhaust2;
	hide rflare2;
	hide rexhaust1;
	hide rflare1;
	hide lexhaust1;
	hide lflare1;
	hide lexhaust2;
	hide lflare2;
	hide aimx1;
	hide aimy1;
	spin thing around x-axis speed <30> accelerate <1>;
	isMoving = FALSE;
	isAiming = FALSE;
	isReady = 1;
	gun_1 = 0;
	animSpeed = 4;
	turn lexhaust1 to z-axis <180> now;
	
	turn lexhaust2 to z-axis <180> now;
	
	turn rexhaust1 to z-axis <180> now;
	turn rexhaust2 to z-axis <180> now;
}


AimFromWeapon1(pieceIndex)
{
	pieceIndex = head;
}

FireWeapon1() {
	gun_1 = 0;
	call-script lua_UnitScriptLight(1, shotcount);
	call-script lua_UnitScriptLight(2, shotcount);
	call-script lua_UnitScriptLight(3, shotcount);
	call-script lua_UnitScriptLight(4, shotcount);
	shotcount = shotcount + 1;
}

Shot1(zero) {

	if (gun_1 == 0) emit-sfx UNIT_SFX0 from lexhaust2;

	if (gun_1 == 1)	emit-sfx UNIT_SFX0 from lexhaust1;
	if (gun_1 == 2) emit-sfx UNIT_SFX0 from rexhaust1;

	if (gun_1 == 3) emit-sfx UNIT_SFX0 from rexhaust2;

	sleep 32;
	// sleep frame, to update gun flare frame AFTER it fires
	++gun_1;
	if (gun_1 == 4) 
	{
	gun_1 = 0;
	}
	
}

QueryWeapon1(pieceIndex) {

	pieceIndex = lflare1 + gun_1;

}



