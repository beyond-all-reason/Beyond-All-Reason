	turn lfoot to x-axis <27> speed <202.500000>;

	move pelvis to y-axis [3] now;
	turn pelvis to x-axis <1> speed <7.500000>;
	turn pelvis to z-axis <-1> speed <7.500000>;

	sleep 125;
