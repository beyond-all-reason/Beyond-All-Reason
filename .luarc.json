{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime.version": "Lua 5.1",
  "completion.requireSeparator": "/",
  "runtime.path": [
      "?",
      "?.lua"
  ],
  "runtime.special": {
      "VFS.Include": "require",
      "include": "require",
      "shard_include": "require"
  },
  "workspace": {
    "library": [
      "recoil-lua-library",
      "types",
      "common/luaUtilities"
    ],
    "ignoreDir": [
      ".vscode",
      "luaui/Tests",
      "luaui/TestsExamples"
    ]
  }
}
